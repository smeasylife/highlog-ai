# Application
APP_NAME=HighLog AI Service
APP_VERSION=1.0.0
DEBUG=True
HOST=0.0.0.0
PORT=8000

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/highlog

# AWS S3
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=ap-northeast-2
AWS_S3_BUCKET=highlog-records

# Google AI (Gemini)
GOOGLE_API_KEY=your_google_api_key

# LangGraph (Optional)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your_langchain_api_key
LANGCHAIN_PROJECT=highlog-interview

# CORS
CORS_ORIGINS=["http://localhost:3000","http://localhost:8080"]
